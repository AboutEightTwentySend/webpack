{
    "name": "react",
    "version": "1.0.0",
    "description": "react+webpack",
    "main": "index.js",
    "scripts": {
        "dev": "better-npm-run dev",
        "compile": "better-npm-run compile"
    },
    "keywords": [
        "react",
        "webpack",
        "html"
    ],
    "betterScripts": {
        "dev": {
            "command": "node server.js",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "compile": {
            "command": "webpack",
            "env": {
                "NODE_ENV": "production"
            }
        }
    },
    "author": "Drod",
    "license": "ISC",
    "dependencies": {
        "better-npm-run": "0.0.15",
        "html-webpack-plugin": "^2.28.0",
        "jsx-loader": "^0.13.2",
        "webpack": "^2.3.3",
        "webpack-dev-server": "^2.4.2"
    }
}
